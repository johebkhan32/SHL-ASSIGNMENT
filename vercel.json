{
    "version": 2,
    "builds": [
      { "src": "api/app.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "api/app.py" }
    ],
    "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
    "git": {
      "deploymentEnabled": {
        "main": true
      }
    }
}

  